INSERT INTO person (person_id, person_name, birthday, deathday, biography, profile_path, home_page) VALUES (57116, 'David Johansen', '1/9/1950', NULL, 'David Roger Johansen (born January 9, 1950) is an American rock, protopunk, blues, and pop singer, songwriter and actor. He is best known as a member of the seminal protopunk band The New York Dolls. He is also known for his work under the pseudonym Buster Poindexter.  David has appeared in several films including Car 54, Where Are You? and Scrooged with Bill Murray.', 'https://image.tmdb.org/t/p/w185/vWhq871aXCZfW3NpYILRxCI86nX.jpg', NULL);
