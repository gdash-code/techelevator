INSERT INTO person (person_id, person_name, birthday, deathday, biography, profile_path, home_page) VALUES (50744, 'Olga Karlatos', '4/20/1947', NULL, 'Olga Karlatos (Greek: Όλγα Καρλάτου; born 20 April 1947) is a Greek actress known primarily for performing in Italian horror cinema .Karlatos is best known for performances in such films as Zombi 2, Murder Rock, Purple Rain, My Friends and Once Upon A Time In America. In 2007 Karlatos graduated from Kent University with a law degree and was admitted to the Bermuda Bar Association in 2010. She moved to Bermuda with her husband, famous animator Arthur Rankin Jr., and continued living there after his death in 2014.', 'https://image.tmdb.org/t/p/w185/cdsJAabO7RlSzzQzTYv49n245qG.jpg', NULL);
