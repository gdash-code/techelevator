INSERT INTO person (person_id, person_name, birthday, deathday, biography, profile_path, home_page) VALUES (7040, 'Kario Salem', '5/23/1955', NULL, 'Kario Salem (born May 23, 1955), is an American television, film, and stage actor and screenwriter.In 1997, Salem earned an Emmy Award as a writer for the television special Don King: Only in America, which also earned him a PEN nomination.', 'https://image.tmdb.org/t/p/w185/mAKOgOVfX0Bih7TADUrIe0jqivc.jpg', NULL);
