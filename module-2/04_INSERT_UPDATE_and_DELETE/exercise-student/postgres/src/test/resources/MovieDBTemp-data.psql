INSERT INTO person (person_id, person_name, birthday, deathday, biography, profile_path, home_page) VALUES (1542, 'Willie Garson', '2/20/1964', NULL, 'William Garson Paszamant (born February 20, 1964) is an American actor. He has appeared in over 75 films, and more than 300 TV episodes. He is known for playing Stanford Blatch on the HBO series Sex and the City and in the related films Sex and the City and Sex and the City 2, and for his role as Mozzie, in the USA Network series White Collar from 2009 to 2014. He appears on Hawaii Five-0 as Gerard Hirsch.', 'https://image.tmdb.org/t/p/w185/lsODqbgT4mpzXHxf8MWCYOZGULc.jpg', NULL);
