INSERT INTO person (person_id, person_name, birthday, deathday, biography, profile_path, home_page) VALUES (1386089, 'Edward Snowden', '6/21/1983', NULL, 'Edward Snowden is an American computer professional, former Central Intelligence Agency (CIA) employee and former contractor for the United States government who copied and leaked classified information from the National Security Agency (NSA) in 2013 without prior authorization. His disclosures revealed numerous global surveillance programs, many run by the NSA and the Five Eyes Intelligence Alliance with the cooperation of telecommunication companies and European governments.', 'https://image.tmdb.org/t/p/w185/q2n2J4HVxty15I9vPJ4j8yX6gdu.jpg', 'https://www.edwardsnowden.com/');
INSERT INTO person (person_id, person_name, birthday, deathday, biography, profile_path, home_page) VALUES (1520071, 'Mike Radford', NULL, NULL, 'Mike Radford is a television executive producer based in London, England. He began working for BBC Current Affairs as a reporter and producer in the early 2000s. In 2004, Radford joined the Channel 4 News Independents Fund as an Executive Producer, and he was promoted to Commissioning Editor in 2009.', 'https://image.tmdb.org/t/p/w185/muH3PwEKwiLZ4qzpIEGMChOsqFa.jpg', NULL);
