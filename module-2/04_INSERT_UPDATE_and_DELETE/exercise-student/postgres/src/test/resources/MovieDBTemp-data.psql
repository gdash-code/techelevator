INSERT INTO person (person_id, person_name, birthday, deathday, biography, profile_path, home_page) VALUES (529, 'Guy Pearce', '10/5/1967', NULL, 'Guy Edward Pearce (born 5 October 1967) is an English-born Australian actor and musician, known for his roles as Leonard Shelby in Christopher Nolan''s Memento, Lieutenant Ed Exley in the film L.A. Confidential, a drag queen in the cult film The Adventures of Priscilla, Queen of the Desert, Mike Young in the popular Australian television series Neighbours and King Edward VIII ("David") in the 2010 Best Picture Oscar winner The King''s Speech.', 'https://image.tmdb.org/t/p/w185/vTqk6Nh3WgqPubkS23eOlMAwmwa.jpg', 'http://www.guypearce.net/');
