INSERT INTO person (person_id, person_name, birthday, deathday, biography, profile_path, home_page) VALUES (6944, 'Octavia Spencer', '5/25/1972', NULL, 'Octavia Lenora Spencer is an American actress, author, and producer. She is the recipient of several accolades, including an Academy Award, a British Academy Film Award, a Golden Globe Award, and three Screen Actors Guild Awards. She is one of two black actresses to have received three Academy Award nominations, the first black actress to receive two consecutive Academy Award nominations in back-to-back years, and the first black actress to receive an Academy Award nomination after previously winning.', 'https://image.tmdb.org/t/p/w185/jnQTP4RRkoWnyO3yL2PgRHZi0tK.jpg', NULL);
